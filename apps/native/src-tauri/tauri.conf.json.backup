{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "prompalette-staging",
  "version": "0.1.2",
  "identifier": "com.prompalette.app.staging",
  "build": {
    "beforeDevCommand": "pnpm dev:web",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "PromPalette [STAGING]"
      }
    ],
    "security": {
      "csp": {
        "default-src": [
          "'self'"
        ],
        "script-src": [
          "'self'"
        ],
        "style-src": [
          "'self'"
        ],
        "img-src": [
          "'self'",
          "data:",
          "https:"
        ],
        "font-src": [
          "'self'"
        ],
        "connect-src": [
          "'self'",
          "tauri:"
        ],
        "object-src": [
          "'none'"
        ],
        "base-uri": [
          "'self'"
        ],
        "form-action": [
          "'self'"
        ],
        "frame-ancestors": [
          "'none'"
        ]
      },
      "dangerousDisableAssetCspModification": false
    }
  },
  "plugins": {
    "updater": {
      "active": false,
      "endpoints": [
        "https://api.github.com/repos/Jun-T-git/prompalette/releases/latest"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDVFNzRCRDIyNjM4OTY1RDQKUldUVVpZbGpJcjEwWGxJUHRmUCtWSWVKd2I4Uk9yR2M5cFI3THc0eEQwdHZ2ZEI2d25wNVEwVjEK"
    }
  },
  "bundle": {
    "active": true,
    "targets": [
      "dmg"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "frameworks": [],
      "minimumSystemVersion": "10.15",
      "entitlements": null,
      "exceptionDomain": "",
      "signingIdentity": null,
      "providerShortName": null,
      "dmg": {
        "appPosition": {
          "x": 180,
          "y": 170
        },
        "applicationFolderPosition": {
          "x": 480,
          "y": 170
        },
        "windowSize": {
          "width": 660,
          "height": 400
        }
      }
    }
  }
}